{
  "name": "troika",
  "version": "0.10.5",
  "description": "Troika - a framework for managing Three.js scenes using point-in-time descriptors. Includes support for automatic transitions, animations, and pointer events.",
  "author": "Jason Johnston <jason.johnston@protectwise.com>",
  "main": "build/troika.js",
  "module": "src/index.js",
  "jsnext:main": "build/troika.esmodule.js",
  "dependencies": {
    "easingjs": "^1.0.1",
    "prop-types": "^15.5.10",
    "three": "0.97.x"
  },
  "engines": {
    "node": "=8.9.4",
    "npm": "=5.8.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.7.3",
    "adaptive-bezier-curve": "^1.0.3",
    "babel": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "buble": "^0.19.3",
    "d3-hierarchy": "^1.1.8",
    "d3-voronoi": "^1.1.4",
    "esdoc": "^1.1.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rollup": "^0.66.3",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.6.0",
    "rollup-plugin-string": "^2.0.2",
    "rollup-watch": "^4.3.1",
    "standard": "^12.0.1",
    "three-line-2d": "^1.1.6"
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -w -c",
    "test": "jest",
    "examples": "rollup -w -c rollup.config.examples.js",
    "docs": "esdoc",
    "clean": "rm -rf build",
    "lint": "standard src/**/*.js src/**/*.jsx"
  },
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "includes": [
      "\\.jsx?$"
    ],
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      },
      {
        "name": "esdoc-jsx-plugin"
      }
    ]
  },
  "publishConfig": {
    "registry": "https://internal-npm/content/repositories/npm/"
  }
}
