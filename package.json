{
  "name": "troika",
  "version": "0.5.2",
  "description": "Troika - a framework for managing Three.js scenes using point-in-time descriptors. Includes support for automatic transitions, animations, and pointer events.",
  "author": "Jason Johnston <jason.johnston@protectwise.com>",
  "main": "build/troika.js",
  "jsnext:main": "build/troika.esmodule.js",
  "dependencies": {
    "easingjs": "^1.0.1",
    "three": "0.86.x"
  },
  "peerDependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "adaptive-bezier-curve": "^1.0.3",
    "d3-hierarchy": "^1.0.2",
    "d3-voronoi": "^1.1.0",
    "lodash": "^4.16.6",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "rollup": "^0.36.3",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-serve": "^0.1.0",
    "rollup-plugin-string": "^2.0.2",
    "rollup-watch": "^2.5.0",
    "three-line-2d": "^1.1.6",
    "uglify-js": "^2.7.3",
    "whatwg-fetch": "latest"
  },
  "scripts": {
    "build": "rollup -c",
    "build-compressed": "rollup -c && uglifyjs --compress --mangle -- build/troika.js > build/troika.min.js",
    "watch": "rollup -w -c",
    "examples": "rollup -w -c rollup.config.examples.js",
    "clean": "rm -rf build"
  },
  "publishConfig": {
    "registry": "https://internal-npm/content/repositories/npm/"
  }
}
